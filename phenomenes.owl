@prefix : <http://www.semanticweb.org/hrawsthorne/ontologies/2021/2/untitled-ontology-8#> .
@prefix gem: <http://www.eionet.europa.eu/gemet/concept/> .
@prefix nav: <http://data.shom.fr/def/navigation_cotiere#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix tcr: <http://data.shom.fr/id/codes/nav/typedecourant/> .
@prefix tim: <http://data.shom.fr/id/codes/nav/typedindicationmeteorologique/> .
@prefix tio: <http://data.shom.fr/id/codes/nav/typedindicationoceanographique/> .
@prefix tit: <http://data.shom.fr/id/codes/nav/typedindicationtemporelle/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix pext: <http://www.ontotext.com/proton/protonext#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix weat: <https://bimerr.iot.linkeddata.es/def/weather#> .
@prefix sophod: <http://sweetontology.net/phenOceanDynamics/> .
@prefix sophatmo: <http://sweetontology.net/phenAtmo/> .
@prefix sophatmow: <http://sweetontology.net/phenAtmoWind/> .
@base <http://www.semanticweb.org/hrawsthorne/ontologies/2021/2/untitled-ontology-8> .

<http://www.semanticweb.org/hrawsthorne/ontologies/2021/2/untitled-ontology-8> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#related
skos:related rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://data.shom.fr/def/navigation_cotiere#aPourConditionLocaleQualitative
nav:aPourConditionLocaleQualitative rdf:type owl:ObjectProperty ;
                                    rdfs:range nav:ConditionLocale ;
                                    rdfs:comment "L'objet de cette propriété peut être une valeur qui nécessite être précisé avec une autre propriété et objet. Donc, elle peut être dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourConditionLocale _:objet>> _:autrePropriete _:autreObjet."@fr ;
                                    rdfs:label "a pour condition locale qualitative"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourConditionMeteo
nav:aPourConditionMeteo rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf nav:aPourConditionLocaleQualitative ;
                        rdfs:range nav:IndicationMeteo ;
                        rdfs:comment "L'objet de cette propriété peut être une valeur qui nécessite être précisé avec une autre propriété et objet. Donc, elle peut être dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourConditionLocale _:objet>> _:autrePropriete _:autreObjet."@fr ;
                        rdfs:label "a pour condition météorologique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourConditionOceano
nav:aPourConditionOceano rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf nav:aPourConditionLocaleQualitative ;
                         rdfs:range nav:IndicationOceano ;
                         rdfs:comment "L'objet de cette propriété peut être une valeur qui nécessite être précisé avec une autre propriété et objet. Donc, elle peut être dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourConditionLocale _:objet>> _:autrePropriete _:autreObjet."@fr ;
                         rdfs:label "a pour condition océanographique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourDirection
nav:aPourDirection rdf:type owl:ObjectProperty ;
                   rdfs:domain nav:PhenomeneMeteorologiqueOuOceanographique ;
                   rdfs:range nav:DirectionCardinale ;
                   rdfs:comment "Exemple : vent aPourDirection NE = vent de NE = vient du NE, va vers SW"@fr ;
                   rdfs:label "a pour direction"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourPortDeRattachement
nav:aPourPortDeRattachement rdf:type owl:ObjectProperty ;
                            rdfs:domain nav:CourantDeMaree ;
                            rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                            rdfs:label "a pour port de rattachement"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourTemporaliteQualitative
nav:aPourTemporaliteQualitative rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf nav:aPourConditionLocaleQualitative ;
                                rdfs:range nav:IndicationTemporelle ;
                                rdfs:label "a pour temporalité qualitative"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourTypeDeCourant
nav:aPourTypeDeCourant rdf:type owl:ObjectProperty ;
                       rdfs:domain nav:Courant ;
                       rdfs:range [ rdf:type owl:Restriction ;
                                    owl:onProperty skos:inScheme ;
                                    owl:hasValue <https://data.shom.fr/id/codes/navigation_cotiere/typedecourant/liste>
                                  ] ;
                       rdfs:label "a pour type de courant"@fr .


###  http://data.shom.fr/def/navigation_cotiere#apres
nav:apres rdf:type owl:ObjectProperty ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationmeteorologique/liste>
                                     ]
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationoceanographique/liste>
                                     ]
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationtemporelle/liste>
                                     ]
                                   )
                     ] .


###  http://data.shom.fr/def/navigation_cotiere#avant
nav:avant rdf:type owl:ObjectProperty ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationmeteorologique/liste>
                                     ]
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationoceanographique/liste>
                                     ]
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty skos:inScheme ;
                                       owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationtemporelle/liste>
                                     ]
                                   )
                     ] .


###  http://data.shom.fr/def/navigation_cotiere#estLocaliseA
nav:estLocaliseA rdf:type owl:ObjectProperty ;
                 rdfs:domain nav:PhenomeneMeteorologiqueOuOceanographique ;
                 rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                 rdfs:label "est localisé à"@fr .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:ObjectProperty ;
                rdfs:label "exact match"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty ;
              rdfs:label "is in scheme"@en .


#################################################################
#    Data properties
#################################################################

###  http://data.shom.fr/def/navigation_cotiere#aPourCap
nav:aPourCap rdf:type owl:DatatypeProperty ;
             rdfs:domain nav:PhenomeneMeteorologiqueOuOceanographique ;
             rdfs:range xsd:float ;
             rdfs:comment "L'objet de cette propriété sera toujours une valeur avec une unité de mesure associée. Donc, elle sera toujours dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourCap \"valeur\"^^xsd:float>> qudt:Unit _:UnitéDeMesure."@fr ;
             rdfs:label "a pour cap"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourConditionLocaleQuantitative
nav:aPourConditionLocaleQuantitative rdf:type owl:DatatypeProperty ;
                                     rdfs:range xsd:string .


###  http://data.shom.fr/def/navigation_cotiere#aPourHoraire
nav:aPourHoraire rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf nav:aPourTemporaliteQuantitative ;
                 rdfs:range xsd:time .


###  http://data.shom.fr/def/navigation_cotiere#aPourTemporaliteQuantitative
nav:aPourTemporaliteQuantitative rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf nav:aPourConditionLocaleQuantitative ;
                                 rdfs:range xsd:string .


###  http://data.shom.fr/def/navigation_cotiere#aPourTempsApresPM
nav:aPourTempsApresPM rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf nav:aPourTemporaliteQuantitative ;
                      rdfs:range xsd:time .


###  http://data.shom.fr/def/navigation_cotiere#aPourTempsAvantPM
nav:aPourTempsAvantPM rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf nav:aPourTemporaliteQuantitative ;
                      rdfs:range xsd:time .


###  http://data.shom.fr/def/navigation_cotiere#aPourVitesse
nav:aPourVitesse rdf:type owl:DatatypeProperty ;
                 rdfs:domain nav:PhenomeneMeteorologiqueOuOceanographique ;
                 rdfs:range xsd:float ;
                 rdfs:comment "L'objet de cette propriété sera toujours une valeur avec une unité de mesure associée. Donc, elle sera toujours dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourVitesse \"valeur\"^^xsd:float>> qudt:Unit _:UnitéDeMesure."@fr ;
                 rdfs:label "a pour vitesse"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourVitesseMaxi
nav:aPourVitesseMaxi rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf nav:aPourVitesse ;
                     rdfs:range xsd:float ;
                     rdfs:comment "L'objet de cette propriété sera toujours une valeur avec une unité de mesure associée. Donc, elle sera toujours dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourVitesse \"valeur\"^^xsd:float>> qudt:Unit _:UnitéDeMesure."@fr ;
                     rdfs:label "a pour vitesse maximum"@fr .


###  http://data.shom.fr/def/navigation_cotiere#aPourVitesseMini
nav:aPourVitesseMini rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf nav:aPourVitesse ;
                     rdfs:range xsd:float ;
                     rdfs:comment "L'objet de cette propriété sera toujours une valeur avec une unité de mesure associée. Donc, elle sera toujours dans un quoted triple écrit en RDF-star. Par exemple : <<_:entité _:aPourVitesse \"valeur\"^^xsd:float>> qudt:Unit _:UnitéDeMesure."@fr ;
                     rdfs:label "a pour vitesse minimum"@fr .


#################################################################
#    Classes
#################################################################

###  http://data.shom.fr/def/navigation_cotiere#ConditionLocale
nav:ConditionLocale rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty nav:aPourConditionLocaleQualitative ;
                                          owl:someValuesFrom nav:TypeDeConditionLocale
                                        ] .


###  http://data.shom.fr/def/navigation_cotiere#Courant
nav:Courant rdf:type owl:Class ;
            owl:equivalentClass pext:WaterCurrent ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty nav:aPourTypeDeCourant ;
                                  owl:someValuesFrom nav:TypeDeCourant
                                ] ;
            rdfs:subClassOf nav:PhenomeneOceanographique ;
            rdfs:comment "Le mouvement permanent ou périodique des eaux, généralement dans le sens horizontal, dû à diverses causes telles que des différences de température, le vent ou les marées."@fr ;
            rdfs:label "Courant"@fr .


###  http://data.shom.fr/def/navigation_cotiere#CourantDeMaree
nav:CourantDeMaree rdf:type owl:Class ;
                   owl:equivalentClass sophod:TidalCurrent ;
                   rdfs:subClassOf nav:Courant ;
                   rdfs:label "Courant de Marée"@fr .


###  http://data.shom.fr/def/navigation_cotiere#CourantDuAuVent
nav:CourantDuAuVent rdf:type owl:Class ;
                    rdfs:subClassOf nav:Courant ;
                    rdfs:label "Courant dû au Vent"@fr .


###  http://data.shom.fr/def/navigation_cotiere#DirectionCardinale
nav:DirectionCardinale rdf:type owl:Class ;
                       rdfs:isDefinedBy nav: ;
                       rdfs:label "Cardinal Direction"@en ,
                                  "Direction Cardinale"@fr .


###  http://data.shom.fr/def/navigation_cotiere#EntiteGeographique
nav:EntiteGeographique rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.opengis.net/ont/geosparql#Feature> .


###  http://data.shom.fr/def/navigation_cotiere#Etale
nav:Etale rdf:type owl:Class ;
          owl:equivalentClass nav:Renverse ;
          rdfs:subClassOf nav:CourantDeMaree ;
          rdfs:comment "Entre le flot et le jusant, le courant devient nul durant une période variable selon le lieu : c'est l'étale ou renverse."@fr ;
          rdfs:label "Étale"@fr .


###  http://data.shom.fr/def/navigation_cotiere#Flot
nav:Flot rdf:type owl:Class ;
         rdfs:subClassOf nav:CourantDeMaree ;
         rdfs:comment "Le courant de flot est le courant créé par la marée montante. Le flot ne coïncide pas exactement avec la marée montante."@fr ;
         rdfs:label "Flot"@fr .


###  http://data.shom.fr/def/navigation_cotiere#IndicationMeteo
nav:IndicationMeteo rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty nav:aPourConditionMeteo ;
                                          owl:someValuesFrom nav:TypeDIndicationMeteorologique
                                        ] ;
                    rdfs:subClassOf nav:ConditionLocale ;
                    rdfs:label "Indication Météorologique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#IndicationOceano
nav:IndicationOceano rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty nav:aPourConditionOceano ;
                                           owl:someValuesFrom nav:TypeDIndicationOceanographique
                                         ] ;
                     rdfs:subClassOf nav:ConditionLocale ;
                     rdfs:label "Indication Océanographique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#IndicationTemporelle
nav:IndicationTemporelle rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty nav:aPourTemporaliteQualitative ;
                                               owl:someValuesFrom nav:TypeDIndicationTemporelle
                                             ] ;
                         rdfs:subClassOf nav:ConditionLocale ;
                         rdfs:label "Indication Temporelle"@fr .


###  http://data.shom.fr/def/navigation_cotiere#Jusant
nav:Jusant rdf:type owl:Class ;
           rdfs:subClassOf nav:CourantDeMaree ;
           rdfs:comment "Le courant de jusant est le courant créé par la marée descendante. Le le jusant ne coïncide pas exactement avec la marée descendante."@fr ;
           rdfs:label "Jusant"@fr .


###  http://data.shom.fr/def/navigation_cotiere#PhenomeneMeteorologique
nav:PhenomeneMeteorologique rdf:type owl:Class ;
                            owl:equivalentClass sophatmo:MeteorologicalPhenomena ;
                            rdfs:subClassOf nav:PhenomeneMeteorologiqueOuOceanographique .


###  http://data.shom.fr/def/navigation_cotiere#PhenomeneMeteorologiqueOuOceanographique
nav:PhenomeneMeteorologiqueOuOceanographique rdf:type owl:Class ;
                                             rdfs:label "Phénomène Météorologique ou Océanographique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#PhenomeneOceanographique
nav:PhenomeneOceanographique rdf:type owl:Class ;
                             rdfs:subClassOf nav:PhenomeneMeteorologiqueOuOceanographique .


###  http://data.shom.fr/def/navigation_cotiere#Renverse
nav:Renverse rdf:type owl:Class ;
             rdfs:subClassOf nav:CourantDeMaree ;
             rdfs:comment "Entre le flot et le jusant, le courant devient nul durant une période variable selon le lieu : c'est l'étale ou renverse."@fr ;
             rdfs:label "Renverse"@fr .


###  http://data.shom.fr/def/navigation_cotiere#Ressac
nav:Ressac rdf:type owl:Class ;
           rdfs:subClassOf nav:Vague ;
           rdfs:comment "Aller et retour violent des vagues sur elles-mêmes, lorsqu'elles se brisent contre un obstacle (https://www.cnrtl.fr/definition/ressac)."@fr .


###  http://data.shom.fr/def/navigation_cotiere#TypeDIndicationMeteorologique
nav:TypeDIndicationMeteorologique rdf:type owl:Class ;
                                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                                        owl:onProperty skos:inScheme ;
                                                        owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationmeteorologique/liste>
                                                      ] ;
                                  rdfs:subClassOf nav:TypeDeConditionLocale ;
                                  rdfs:label "Type d'Indication Météorologique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#TypeDIndicationOceanographique
nav:TypeDIndicationOceanographique rdf:type owl:Class ;
                                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                                         owl:onProperty skos:inScheme ;
                                                         owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationoceanographique/liste>
                                                       ] ;
                                   rdfs:subClassOf nav:TypeDeConditionLocale ;
                                   rdfs:label "Type d'Indication Océanographique"@fr .


###  http://data.shom.fr/def/navigation_cotiere#TypeDIndicationTemporelle
nav:TypeDIndicationTemporelle rdf:type owl:Class ;
                              owl:equivalentClass [ rdf:type owl:Restriction ;
                                                    owl:onProperty skos:inScheme ;
                                                    owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedindicationtemporelle/liste>
                                                  ] ;
                              rdfs:subClassOf nav:TypeDeConditionLocale ;
                              rdfs:label "Type d'Indication Temporelle"@fr .


###  http://data.shom.fr/def/navigation_cotiere#TypeDeConditionLocale
nav:TypeDeConditionLocale rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty skos:inScheme ;
                                                owl:hasValue <http://data.shom.fr/id/codes/navigation_cotiere/typedeconditionlocale/liste>
                                              ] ;
                          rdfs:subClassOf skos:Concept ;
                          rdfs:label "Type de Condition Locale"@fr .


###  http://data.shom.fr/def/navigation_cotiere#TypeDeCourant
nav:TypeDeCourant rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty skos:inScheme ;
                                        owl:hasValue <https://data.shom.fr/id/codes/navigation_cotiere/typedecourant/liste>
                                      ] ;
                  rdfs:subClassOf skos:Concept ;
                  rdfs:label "Type de Courant"@fr .


###  http://data.shom.fr/def/navigation_cotiere#Vague
nav:Vague rdf:type owl:Class ;
          rdfs:subClassOf nav:PhenomeneOceanographique .


###  http://data.shom.fr/def/navigation_cotiere#Vent
nav:Vent rdf:type owl:Class ;
         owl:equivalentClass sophatmow:Wind ,
                             weat:Wind ;
         rdfs:subClassOf nav:PhenomeneMeteorologique .


###  http://sweetontology.net/phenAtmo/MeteorologicalPhenomena
sophatmo:MeteorologicalPhenomena rdf:type owl:Class .


###  http://sweetontology.net/phenAtmoWind/Wind
sophatmow:Wind rdf:type owl:Class .


###  http://sweetontology.net/phenOceanDynamics/TidalCurrent
sophod:TidalCurrent rdf:type owl:Class .


###  http://www.ontotext.com/proton/protonext#WaterCurrent
pext:WaterCurrent rdf:type owl:Class .


###  http://www.opengis.net/ont/geosparql#Feature
<http://www.opengis.net/ont/geosparql#Feature> rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#ConceptScheme
skos:ConceptScheme rdf:type owl:Class .


###  https://bimerr.iot.linkeddata.es/def/weather#Wind
weat:Wind rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://data.shom.fr/id/codes/nav/typedecourant/CourantAlternatif
tcr:CourantAlternatif rdf:type owl:NamedIndividual ,
                               nav:TypeDeCourant ;
                      rdfs:comment "Un courant alternatif est un courant qui porte dans une direction au flot et dans la direction opposée pendant le jusant quel que soit le coefficient de marée."@fr ;
                      rdfs:label "courant alternatif"@fr .


###  http://data.shom.fr/id/codes/nav/typedecourant/CourantGiratoire
tcr:CourantGiratoire rdf:type owl:NamedIndividual ,
                              nav:TypeDeCourant ;
                     rdfs:comment "Un courant giratoire est un courant qui porte dans une direction qui varie en fonction de l'heure de la marée (donc en 12 h 25 min)."@fr ;
                     rdfs:label "courant giratoire"@fr .


###  http://data.shom.fr/id/codes/nav/typedecourant/CourantTraversier
tcr:CourantTraversier rdf:type owl:NamedIndividual ,
                               nav:TypeDeCourant ;
                      rdfs:label "courant traversier"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationmeteorologique/Brume
tim:Brume rdf:type owl:NamedIndividual ,
                   nav:TypeDIndicationMeteorologique ;
          skos:inScheme <http://www.eionet.europa.eu/gemet/concept/3348> ;
          rdfs:label "brume"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationmeteorologique/MauvaisTemps
tim:MauvaisTemps rdf:type owl:NamedIndividual ,
                          nav:TypeDIndicationMeteorologique ;
                 rdfs:label "mauvais temps"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationoceanographique/BasseMer
tio:BasseMer rdf:type owl:NamedIndividual ,
                      nav:TypeDIndicationOceanographique ;
             rdfs:label "basse mer"@fr ,
                        "low tide"@en .


###  http://data.shom.fr/id/codes/nav/typedindicationoceanographique/FinDuFlot
tio:FinDuFlot rdf:type owl:NamedIndividual ,
                       nav:TypeDIndicationOceanographique ;
              rdfs:label "fin du flot"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationoceanographique/FinDuJusant
tio:FinDuJusant rdf:type owl:NamedIndividual ,
                         nav:TypeDIndicationOceanographique ;
                rdfs:label "fin du jusant"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationoceanographique/MareeDeMorte-Eau
tio:MareeDeMorte-Eau rdf:type owl:NamedIndividual ,
                              nav:TypeDIndicationOceanographique ;
                     rdfs:comment "En période de mortes-eaux, le marnage est minimal (la mer recouvre et découvre moins) et les courants de marée sont moins forts."@fr ;
                     rdfs:label "marée de morte-eau"@fr .


###  http://data.shom.fr/id/codes/nav/typedindicationoceanographique/MareeDeVive-Eau
tio:MareeDeVive-Eau rdf:type owl:NamedIndividual ,
                             nav:TypeDIndicationOceanographique ;
                    rdfs:comment "En période de vives-eaux, le marnage est plus grand (la mer recouvre et découvre plus) et les courants de marée sont plus forts."@fr ;
                    rdfs:label "marée de vive-eau"@fr .


